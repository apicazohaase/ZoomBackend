/*
Model of all the participants of the application
*/

namespace zoom.app.participants


participant Business identified by id {
  o String id
  o String name
  o String password
  o String confirmPassword
  o String mail
  o Integer number optional
  o String city optional
  o String localidad optional
  o String street optional
  o TypeParticipant participante
}

enum TypeParticipant {
    o VENDOR
    o CLIENT
    o TRANSPORT
}